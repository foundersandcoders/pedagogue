<Module>
  <Metadata>
    <!-- OPTIONAL: Generation tracking and change log -->
    <!-- This section documents what changed, why, and with what confidence -->
    <!-- Enables safe cascade pattern maintenance with human oversight -->
    <GenerationInfo>
      <Timestamp>ISO 8601 datetime of generation</Timestamp>
      <Source>AI-Generated | Human-Reviewed | Hybrid</Source>
      <Model>Model identifier used for generation</Model>
      <InputSources>
        <InputFile type="projects">Filename or identifier</InputFile>
        <InputFile type="skills">Filename or identifier</InputFile>
        <InputFile type="research">Filename or identifier</InputFile>
      </InputSources>
    </GenerationInfo>

    <Changelog>
      <!-- Documents specific changes made during this generation -->
      <Change> <!-- Repeat for each significant change -->
        <Section>XPath to changed section (e.g. ModuleObjectives/Objective[1])</Section>
        <Type>content_update | examples_expanded | new_content | removed | reordered</Type>
        <Confidence>high | medium | low</Confidence>
        <Summary>One sentence description of change</Summary>
        <Rationale>1-3 sentences explaining why this change was made</Rationale>
        <ResearchSources>
          <Source url="">Title/description of research source</Source>
        </ResearchSources>
      </Change>
    </Changelog>

    <ProvenanceTracking>
      <!-- Tracks lineage for cascade pattern -->
      <LastHumanReview>
        <Date>ISO 8601 date</Date>
        <Reviewer>Identifier (e.g., "Council Week 5")</Reviewer>
      </LastHumanReview>
      <AIUpdateCount>Number of AI generations since last human review</AIUpdateCount>
      <SectionsNeedingReview>
        <!-- Sections flagged for human attention (typically low confidence changes) -->
        <Section confidence="low">XPath to section needing review</Section>
      </SectionsNeedingReview>
    </ProvenanceTracking>
  </Metadata>

  <ModuleOverview>
    <ModuleDescription>
      What topics this module covers and what we'll build
    </ModuleDescription>

    <ModuleObjectives>
      <!-- There should be at least 3 tangibly separate objectives -->
      <ModuleObjective> <!-- Repeat <Objective> for each objective -->
        <Name>Quick memorable name for the objective</Name>
        <Details>
          An explanation of the practical skills &amp; theoretical knowledge learners will have upon completion
        </Details>
      </ModuleObjective>
    </ModuleObjectives>
  </ModuleOverview>

  <ResearchTopics>
    <PrimaryTopics>
      <!-- There should be at least 5 distinct primary topics -->
      <!-- They should relate to the content of <Projects> & <ModuleOverview> -->
      <PrimaryTopic> <!-- Repeat <PrimaryTopic> for each primary topic -->
        <TopicName></TopicName>
        
        <TopicDescription>
          Description of topic to research, including:
          1. guidance for how to start researching it
          2. suggestion for subdividing the topic if multiple learners tackle it
        </TopicDescription>
      </PrimaryTopic>
    </PrimaryTopics>

    <StretchTopics>
      <!-- Stretch topics should be suggested if they are natural extensions of Primary Topics -->
      <!-- They must not be essential to completing the briefs in <Projects> -->
      <StretchTopic> <!-- Repeat <StretchTopic> for each stretch topic -->
        One sentence description of topic
      </StretchTopic>
    </StretchTopics>
  </ResearchTopics>

  <Projects>
    <ProjectBriefs>
      <!-- There should be at least 2 distinct briefs -->
      <ProjectBrief> <!-- Repeat <Project> for each project brief -->
        <Overview>
          <Name>Name of Project</Name>
          
          <Task>
            One sentence description of successful outcome
          </Task>
          
          <Focus>
            Techniques &amp; technologies that will help achieve the task
          </Focus>
        </Overview>

        <Criteria>
          Bullet point list of success criteria
        </Criteria>

        <Skills>
          <!-- There should be at least 3 distinct skills, technologies or techniques -->
          <!-- Each skill should have been referenced in <Module/Objectives> -->
          <Skill> <!-- Repeat <Skill> for all skills -->
           	<Name>Memorable name of skill</Name>
            
            <Details>
              Bullet point list of criteria, guidance and explanation
            </Details>
          </Skill>
        </Skills>

        <Examples>
          <!-- There should be at least 3 significantly different examples -->
          <Example> <!-- Repeat <Example> for each example -->
            <Name>Memorable name of example</Name>
            
            <Description>
              Brief description of example
            </Description>
          </Example>
        </Examples>
      </ProjectBrief>
    </ProjectBriefs>

    <ProjectTwists>
      <!-- There should be at least 2 interesting twists -->
      <ProjectTwist> <!-- Repeat <Twist> for each twist -->
        <Name>Memorable name of twist</Name>
        
        <Task>
          Challenge that the twist poses
        </Task>
        
        <ExampleUses>
          <!-- There should be at least 2 distinct examples of incorporating the twist -->
          <Example> <!-- Repeat <Example> for each example -->
            Brief description of example
          </Example>
        </ExampleUses>
      </ProjectTwist>
    </ProjectTwists>
  </Projects>

  <AdditionalSkills>
    <!-- Skills that are not vital but can supplement the main module focus
      Skills should be granular
      Examples:
        Good: "Package management in Python"
        Bad: "Coding in Python"
      -->
    <SkillsCategory> <!-- Repeat <Category> for each category -->
      <Name>Category Name (e.g. "Python")</Name>

      <Skill>
        <SkillName>Memorable name of skill</SkillName>
        <Importance>e.g. Recommended, Stretch etc</Importance>
        
        <SkillDescription>
          2 sentences maximum
        </SkillDescription>
      </Skill>
    </SkillsCategory>
  </AdditionalSkills>

  <Notes>
    Any content that doesn't fit within the schema. Use rarely.
  </Notes>
</Module>
