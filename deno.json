{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run --watch=src/ main.ts",
    "build": "deno run --allow-all build.ts",
    "preview": "deno run --allow-net --allow-read preview.ts"
  },
  "imports": {
    "@std/": "https://deno.land/std@0.224.0/",
    "svelte": "npm:svelte@^4.0.0",
    "@sveltejs/kit": "npm:@sveltejs/kit@^1.20.0",
    "@sveltejs/adapter-node": "npm:@sveltejs/adapter-node@^1.3.0",
    "@sveltejs/vite-plugin-svelte": "npm:@sveltejs/vite-plugin-svelte@^2.4.0",
    "vite": "npm:vite@^4.4.0",
    "langchain": "npm:langchain@^0.1.0",
    "@anthropic-ai/sdk": "npm:@anthropic-ai/sdk@^0.20.0"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["dom", "dom.iterable", "es2022"],
    "allowSyntheticDefaultImports": true,
    "strict": true
  },
  "nodeModulesDir": "auto"
}